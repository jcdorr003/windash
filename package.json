{
  "name": "windash",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "concurrently \"pnpm:dev:app\" \"pnpm:dev:ws\"",
    "dev:app": "react-router dev",
    "dev:ws": "tsx watch app/server/websocket-server.ts",
    "start": "node scripts/start-production.js",
    "start:app": "react-router-serve build/server/index.js",
    "start:ws": "tsx app/server/websocket-server.ts",
    "typecheck": "react-router typegen && tsc",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed.ts",
    "dev:docker": "bash scripts/setup-dev.sh --wait",
    "dev:docker:clean": "bash scripts/setup-dev.sh --wipe-db --rebuild --wait",
    "dev:local": "docker compose -f docker-compose.dev.yml up -d postgres && pnpm db:push && pnpm dev",
    "dev:local:clean": "docker compose -f docker-compose.dev.yml down && docker volume rm postgres-data-dev 2>/dev/null || true && pnpm dev:local",
    "clean:dev": "bash scripts/cleanup.sh --dev",
    "clean:dev:full": "bash scripts/cleanup.sh --dev --with-volumes --images --yes"
  },
  "dependencies": {
  "@react-router/serve": "7.9.2",
  "@remix-run/node-fetch-server": "0.9.0",
  "d3": "^7.9.0",
  "dotenv": "^17.2.3",
  "drizzle-orm": "^0.44.7",
  "postgres": "^3.4.7",
  "react": "19.1.1",
  "react-dom": "19.1.1",
  "react-router": "7.9.2",
  "ws": "^8.18.3"
  },
  "devDependencies": {
    "@react-router/dev": "7.9.2",
    "@react-router/fs-routes": "7.9.2",
    "@tailwindcss/vite": "4.1.13",
    "@types/d3": "^7.4.3",
    "@types/node": "24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-rsc": "0.4.31",
    "concurrently": "^9.2.1",
    "tailwindcss": "4.1.13",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vite-plugin-devtools-json": "1.0.0",
    "vite-tsconfig-paths": "5.1.4",
    "drizzle-kit": "^0.31.6"
  }
}